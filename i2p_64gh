wget http://hacktolive.org/download/super-os-repo-installer.deb https://raw.github.com/cnpirates/futurex.cn/master/i2prouter https://raw.github.com/cnpirates/futurex.cn/master/tahoe.cfg && wget --no-check-certificate https://cloud.github.com/downloads/cnpirates/futurex.cn/wuala_current_amd64.deb && dpkg -i super-os-repo-installer.deb && apt-get -y install python-software-properties && add-apt-repository ppa:i2p.packages/i2p && apt-get update && apt-get -y install i2p i2p-tahoe-lafs i2pbote i2p-messenger robert getdeb-repository wipe terminator geany && dpkg -i wuala_current_amd64.deb && mv i2prouter /usr/bin/ && chmod +x /usr/bin/i2prouter && i2prouter start && tahoe create-client && mv tahoe.cfg /root/.tahoe && tahoe start && wipe -frq ~/.bash_history /var/log/* && history -c && exit
