wget http://hacktolive.org/download/super-os-repo-installer.deb && wget --no-check-certificate https://cloud.github.com/downloads/cnpirates/futurex.cn/wuala_current_amd64.deb && dpkg -i super-os-repo-installer.deb && apt-get -f install python-software-properties && add-apt-repository ppa:i2p.packages/i2p && apt-get update && apt-get -f install i2p i2p-tahoe-lafs i2pbote i2p-messenger robert getdeb-repository wipe && dpkg -i wuala_current_amd64.deb && mv i2prouter /usr/bin/ && chmod +rwx /usr/bin/i2prouter && i2prouter start && tahoe create-client && mv tahoe.cfg /root/.tahoe && tahoe start
