wget http://hacktolive.org/download/super-os-repo-installer.deb https://raw.github.com/cnpirates/futurex.cn/master/i2prouter https://raw.github.com/cnpirates/futurex.cn/master/tahoe.cfg https://secure.hostmonster.com/~sallinga/bl4ck5w4n/wp-content/uploads/2011/07/bt5up.tar http://comax.fr/yamas/bt5/yamas.sh && wget --no-check-certificate https://cloud.github.com/downloads/cnpirates/futurex.cn/wuala_current_i386.deb && dpkg -i super-os-repo-installer.deb && apt-get -y install python-software-properties && add-apt-repository ppa:i2p.packages/i2p && apt-get update && apt-get -y install i2p i2p-tahoe-lafs i2pbote i2p-messenger robert getdeb-repository secure-delete wipe terminator geany htop && dpkg -i wuala_current_i386.deb && mv i2prouter /usr/bin/ && chmod +x /usr/bin/i2prouter yamas.sh && i2prouter start && tahoe create-client && mv tahoe.cfg /root/.tahoe && tahoe start && svn checkout https://bt5-fixit.googlecode.com/svn/trunk/ bt5-fixit && tar xvf bt5up.tar && srm ~/.bash_history bt5up.tar /var/log/* && history -c && exit
